{
  "tools": {
    "read": false,
    "edit": false,
    "bash": false,
    "search": false,
    "list": false,
    "webfetch": false,
    "task_tracker": false,
    "exit_criteria_checker": false,
    "branch_strategy_enforcer": false,
    "agent_handoff_validator": false,
    "type_check_aggregator": false,
    "coverage_analyzer": false,
    "test_runner_smart": false,
    "fixture_generator": false,
    "code_complexity_scorer": false,
    "docstring_validator": false,
    "api_diff_reporter": false,
    "dependency_auditor": false,
    "changelog_generator": false,
    "smart_commit_builder": false,
    "flakiness_detector": false,
    "github_*": false
  },
  "mcp": {
    "github": {
      "type": "remote",
      "url": "https://api.githubcopilot.com/mcp/",
      "oauth": false,
      "headers": {
        "Authorization": "Bearer {env:GITHUB_MCP_PAT}"
      },
      "enabled": false
    },
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "oauth": false,
      "enabled": false,
      "headers": {
        "Authorization": "Bearer {env:CONTEXT7_API_KEY}"
      }
    },
    "motherduck": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@motherduckdb/mcp-server-motherduck"
      ],
      "enabled": false,
      "timeout": 10000,
      "environment": {
        "MOTHERDUCK_TOKEN": "{env:MOTHERDUCK_TOKEN}"
      },
      "note": "Query and analyze data with MotherDuck and local DuckDB. For cloud features, set MOTHERDUCK_TOKEN environment variable. Works locally without token for file-based DuckDB."
    },
    "filesystem": {
      "type": "local",
      "command": [
        "npx",
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "."
      ],
      "enabled": false,
      "timeout": 5000
    },
    "code-reasoning": {
      "type": "local",
      "command": [
        "node",
        "/home/dscv/Repositories/mcp-sequentialthinking-tools/dist/index.js"
      ],
      "enabled": false,
      "environment": {
        "MAX_HISTORY_SIZE": "1000"
      }
    }
  }
}